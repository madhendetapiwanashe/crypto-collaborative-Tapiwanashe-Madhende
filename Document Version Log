import hashlib
import time

class DocumentVersion:
def **init**(self, version, previous_hash, timestamp, editor, changes, hash):
self.version = version
self.previous_hash = previous_hash
self.timestamp = timestamp
self.editor = editor
self.changes = changes
self.hash = hash

def calculate_version_hash(version, previous_hash, timestamp, editor, changes):
value = f"{version}{previous_hash}{timestamp}{editor}{changes}"
return hashlib.sha256(value.encode("utf-8")).hexdigest()

def create_initial_version():
return DocumentVersion(1, "0", time.time(), "Admin", "Initial document created",
calculate_version_hash(1, "0", time.time(), "Admin", "Initial document created"))

doc = create_initial_version()
print(f"Хеш версии: {doc.hash}")
